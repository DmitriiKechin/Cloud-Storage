    "@types/express-fileupload": "^1.2.2",
    "@types/fs-extra": "^9.0.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/uuid": "^8.3.4",
    "concurrently": "^6.3.0",
    "nodemon": "^2.0.14",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.6",
    "express": "^4.17.1",
