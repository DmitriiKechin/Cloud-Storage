{
  "name": "cloud-storage",
  "version": "1.0.0",
  "description": "simple personal cloud storage",
  "main": "server/build/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "tsc && npm install --prefix client",
    "start-dev": "tsc && npm run build --prefix client && npm run start",
    "start": "node server/build/app.js",
    "server": "nodemon --exec ts-node ./server/src/app.ts",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "deploy": "git add . && git commit -m \"deploy\" && git push heroku +HEAD:master"
  },
  "keywords": [
    "react",
    "node",
    "typeScript"
  ],
  "author": "Dmitrii Kechin <dmitrii.kechin@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "0.0.41",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.2",
    "@types/express-http-proxy": "^1.6.3",
    "@types/fs-extra": "^9.0.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.16",
    "@types/uuid": "^8.3.4",
    "concurrently": "^6.3.0",
    "nodemon": "^2.0.14",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-http-proxy": "^1.6.3",
    "express-validator": "^6.13.0",
    "fs-extra": "^10.0.0",
    "http-proxy-middleware": "^2.0.3",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.3.1",
    "mongoose": "^6.0.12",
    "node-fetch": "^2.0.0",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": "14.17.6"
  }
}
